{
  "name": "HarmonyOne",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf node_modules && rm -rf yarn.lock && rm -rf package-lock.json && rm -rf $TMPDIR/react-* && watchman watch-del-all",
    "clean:ios": "cd ios && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && rm -rf Podfile.lock",
    "push:ios-production": "appcenter codepush release-react -a HarmonyPayment/HPOne-iOS -d Production -t 1.0.1 --description fix-bug",
    "push:ios-staging": "appcenter codepush release-react -a HarmonyPayment/HPOne-iOS -d Staging -t 1.0.2 --description update-report",
    "push-clean:ios-production": "appcenter codepush deployment clear -a HarmonyPayment/HPOne-iOS Production",
    "yarn": "appcenter codepush deployment clear -a HarmonyPayment/HPOne-iOS Staging",
    "push:android-production": "appcenter codepush release-react -a HarmonyPayment/HPOne-Android -d Production -t 1.0.0 --description update",
    "push:android-staging": "appcenter codepush release-react -a HarmonyPayment/HPOne-Android -d Staging -t 1.0.0 --description update",
    "open:android": "open -a '/Applications/Android Studio.app' android",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "build-android": "rm -rf ~/.gradle/caches && cd android && ./gradlew --stop && ./gradlew cleanBuildCache && ./gradlew bundleRelease && cd ..",
    "run:android-staging": "npx react-native run-android --variant=staging",
    "run:android-production": "npx react-native run-android --variant=release",
    "run:ios-staging-device": "npx react-native run-ios --scheme='Staging' --device 'iPhone'",
    "run:ios-staging": "npx react-native run-ios --scheme='Staging' --simulator='iPhone 12 Pro Max'",
    "pod": "npx pod-install",
    "new:screen": "npx ts-node ./scripts/createScreen.js",
    "new:page": "npx ts-node ./scripts/createPage.js",
    "new:component": "npx ts-node ./scripts/createComponent.js",
    "open:iPhone": "open -a Simulator --args -CurrentDeviceUDID  53F9D553-5104-45BD-B8FF-B51315D499EC",
    "pod:clean": "cd ios && pod deintegrate && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && rm -rf Podfile.lock",
    "push-history:ios-staging": "appcenter codepush deployment history -a HarmonyPayment/HPOne-iOS Staging"
  },
