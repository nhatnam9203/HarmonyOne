{
  "name": "HarmonyOne",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write ./src && prettier --check ./src",
    "clean": "rm -rf node_modules && rm -rf yarn.lock && rm -rf $TMPDIR/react-* && watchman watch-del-all",
    "clean:ios": "cd ios && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && rm -rf Podfile.lock",
    "push:ios-production": "appcenter codepush release-react -a HarmonyPayment/harmonypay-iOS -d Production -t 1.0.0 --description fix-bug",
    "push:ios-staging": "appcenter codepush release-react -a HarmonyPayment/harmonypay-iOS -d Staging -t 1.0.0 --description test",
    "push-clean:ios-production": "appcenter codepush deployment clear -a HarmonyPayment/harmonypay-iOS Production",
    "push-clean:ios-staging": "appcenter codepush deployment clear -a HarmonyPayment/harmonypay-iOS Staging",
    "push:android-production": "appcenter codepush release-react -a HarmonyPayment/harmonypay-Android -d Production -t 1.0.0 --description update",
    "push:android-staging": "appcenter codepush release-react -a HarmonyPayment/harmonypay-Android -d Staging -t 1.0.0 --description update",
    "open:android": "open -a '/Applications/Android Studio.app' android",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "build-android": "rm -rf ~/.gradle/caches && cd android && ./gradlew --stop && ./gradlew cleanBuildCache && ./gradlew bundleRelease && cd ..",
    "run:android-staging": "npx react-native run-android --variant=staging",
    "run:android-production": "npx react-native run-android --variant=release",
    "run:ios-staging-device": "npx react-native run-ios --scheme='staging' --device 'iPhone'"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^5.11.11",
    "@react-navigation/material-top-tabs": "^5.3.15",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "axios": "^0.21.1",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-native": "0.64.2",
    "react-native-animatable": "^1.3.3",
    "react-native-calendars": "^1.1264.0",
    "react-native-config": "^1.4.3",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^12.0.3",
    "react-native-modalize": "^2.0.8",
    "react-native-pager-view": "^5.4.0",
    "react-native-paper": "^4.9.2",
    "react-native-reanimated": "^2.2.0",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "react-native-smooth-pincode-input": "^1.0.9",
    "react-native-tab-view": "^3.0.1",
    "react-native-vector-icons": "^8.1.0",
    "react-query": "^3.19.1",
    "react-redux": "^7.2.4",
    "reactotron-react-native": "^5.0.0",
    "reactotron-redux": "^3.1.3",
    "recyclerlistview": "^3.0.5",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/runtime": "^7.14.8",
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/jest": "^26.0.24",
    "@types/react-native": "^0.64.12",
    "@types/react-native-vector-icons": "^6.4.8",
    "@types/react-redux": "^7.1.18",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "babel-jest": "^27.0.6",
    "eslint": "^7.31.0",
    "flow-bin": "^0.113.0",
    "jest": "^27.0.6",
    "jetifier": "^2.0.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.3.2",
    "react-test-renderer": "17.0.2",
    "reactotron-redux-saga": "^4.2.3",
    "typescript": "^4.3.5"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
